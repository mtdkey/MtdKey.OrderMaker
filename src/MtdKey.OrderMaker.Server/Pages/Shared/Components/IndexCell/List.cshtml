@model Core.DocFieldModel

<div>
    @if (Model.Value != null)
    {
        var value = (string)Model.Value;
        var text = Model.ListItems.Where(x => x.Id == value).Select(x => x.Name).FirstOrDefault() ?? string.Empty;
        <text>@text</text>
    }
</div>